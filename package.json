{
  "name": "geometric-calculator",
  "version": "1.0.0",
  "description": "A simple React application for calculating geometric properties. Features include circle diameter calculator, triangle area calculator using Heron's formula, modern Material UI interface, and easy switching between shape calculators. Built with React 18 and Material UI (MUI) v5.",
  "keywords": [
    "react",
    "calculator",
    "geometry",
    "mathematics"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.14.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.23.6",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
